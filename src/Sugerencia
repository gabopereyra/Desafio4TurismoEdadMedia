package Aplicacion;

import java.util.Comparator;

import Atraccion.Atraccion;

public class Sugerencia implements Comparator <Atraccion> {

	@Override
	public int compare(Atraccion o1, Atraccion o2) {
		

		
		if (o1.getCostoAtraccion() < o2.getCostoAtraccion()&& o1.getTiempoNecesario() < o2.getTiempoNecesario()) {
			return 1;
		}
		if (o1.getCostoAtraccion() > o2.getCostoAtraccion()&& o1.getTiempoNecesario() < o2.getTiempoNecesario()) {
			return -1;
		}

		return 0;
	}
	

}
